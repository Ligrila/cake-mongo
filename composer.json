{
    "name": "dilab/cake-mongo",
    "description": "A MongoDB datasource and data mapper for CakePHP 3.0",
    "type": "cakephp-plugin",
    "require": {
        "cakephp/cakephp": "~3.1",
        "mongodb/mongodb": "^1.1"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "dev-master",
        "psr/log": "~1.0",
        "phpunit/phpunit": "~5.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "xu ding",
            "email": "thedilab@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dilab\\CakeMongo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dilab\\CakeMongo\\Test\\": "tests"
        }
    }
}
